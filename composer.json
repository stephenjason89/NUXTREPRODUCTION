{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "beyondcode/laravel-websockets": "^1.9.0",
        "cloudflare/sdk": "^1.2",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "instituteweb/composer-scripts": "^1.0",
        "intervention/image": "^2.5",
        "joselfonseca/lighthouse-graphql-passport-auth": "^6.0",
        "laravel/framework": "^8.12",
        "laravel/passport": "^10.0",
        "laravel/scout": "^v9.2.8",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "league/flysystem-aws-s3-v3": "^1.0.29",
        "league/flysystem-memory": "^1.0.2",
        "league/glide-laravel": "^1.0",
        "matthewbdaly/laravel-etag-middleware": "^1.3",
        "mll-lab/graphql-php-scalars": "^4.0",
        "mll-lab/laravel-graphql-playground": "^2.4",
        "nuwave/lighthouse": "^v5.10.0",
        "owen-it/laravel-auditing": "^12.0",
        "pusher/pusher-php-server": "v4.1.5",
        "spatie/laravel-permission": "^5.1.1",
        "stancl/tenancy": "^3.4",
        "staudenmeir/belongs-to-through": "^2.5",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "yab/laravel-scout-mysql-driver": "^v5.0.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.3",
        "barryvdh/laravel-ide-helper": "^2.7",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "friendsofphp/php-cs-fixer": "^v3.0.0",
        "haydenpierce/class-finder": "^0.4.3",
        "kevincobain2000/laravel-erd": "^1.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3.3",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "tests/Helpers/_functions.php"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "lintfix": [
            "\\InstituteWeb\\ComposerScripts\\ImprovedScriptExecution::apply",
            "tools/php-cs-fixer/vendor/bin/php-cs-fixer fix --path-mode=intersection --verbose --config .php-cs-fixer.dist.php"
        ]
    }
}
