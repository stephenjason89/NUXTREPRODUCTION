{
    "name": "apollosystems",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "php artisan serve",
        "websocket": "php artisan websockets:serve",
        "dev": "nuxi dev",
        "styleguide": "cross-env vue-styleguidist server",
        "siege": "node ./siege/siege.js",
        "build": "nuxi build",
        "styleguide:build": "cross-env vue-styleguidist build",
        "start": "nuxi preview",
        "generate": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js generate",
        "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
        "lint:style": "stylelint **/*.{vue,css} --ignore-path .gitignore",
        "lint": "npm run lint:js && npm run lint:style",
        "lintfix": "eslint --fix --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .gitignore . && stylelint --fix **/*.{vue,css} --ignore-path .gitignore",
        "prepare": "husky install",
        "android": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js generate -s && cd dist && del /S *.gz && del /S *.br & cd .. && npx cap copy android && npx cap open android",
        "ios": "node --max-old-space-size=4096 node_modules/nuxt/bin/nuxt.js generate -s && cd dist && find . -name '*.gz' -delete && find . -name '*.br' -delete && cd .. && npx cap copy ios && npx cap open ios"
    },
    "dependencies": {
        "@capacitor/android": "^3.0.0",
        "@capacitor/cli": "^3.0.0",
        "@capacitor/core": "^3.0.0",
        "@capacitor/ios": "^3.0.0",
        "@nuxtjs/apollo": "^4.0.1-rc.5",
        "@nuxtjs/axios": "^5.13.1",
        "@nuxtjs/proxy": "^2.0.1",
        "@vue/apollo-composable": "^4.0.0-alpha.15",
        "animejs": "^3.2.0",
        "apexcharts": "^3.28.3",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-link-batch-http": "^1.2.14",
        "apollo-link-http": "^1.5.17",
        "apollo3-cache-persist": "^0.14.0",
        "colorize-filter": "^1.0.3",
        "cookie": "^0.4.1",
        "core-js": "^3.10.1",
        "encoding": "^0.1.13",
        "face-api.js": "^0.22.2",
        "graphql-tag": "^2.11.0",
        "izitoast": "^1.4.0",
        "js-cookie": "^3.0.1",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.5.23",
        "leaflet": "^1.7.1",
        "leaflet-geosearch": "^3.3.1",
        "localforage": "^1.9.0",
        "nanoid": "^3.1.22",
        "nuxt-compress": "^5.0.0",
        "nuxt-edge": "latest",
        "nuxt-social-meta": "1.0.0",
        "overlayscrollbars": "^1.13.0",
        "overlayscrollbars-vue": "^0.2.2",
        "peerjs": "^1.3.2",
        "pluralize": "^8.0.0",
        "siege": "^0.2.0",
        "spinkit": "^2.0.1",
        "viewerjs": "^1.10.1",
        "vue-apexcharts": "^1.6.2",
        "vue-excel-xlsx": "^1.2.2",
        "vue-frag": "^1.1.4",
        "vue-lazy-hydration": "^2.0.0-beta.4",
        "vue-tour": "^2.0.0",
        "vue2-dropzone": "^3.6.0",
        "vuetify": "*",
        "vuetify-loader": "^1.7.2",
        "vuex-persistedstate": "^4.0.0-beta.3"
    },
    "devDependencies": {
        "@mdi/js": "^6.1.95",
        "@nuxt/bridge": "npm:@nuxt/bridge-edge",
        "@nuxt/image": "^0.6.0",
        "@nuxt/types": "^2.15.8",
        "@nuxtjs/eslint-config-typescript": "^9.0.0",
        "@nuxtjs/eslint-module": "^3.0.2",
        "@nuxtjs/router": "^1.6.1",
        "@nuxtjs/stylelint-module": "^4.0.0",
        "@nuxtjs/tailwindcss": "^5.0.0",
        "@nuxtjs/vuetify": "^1.11.3",
        "@types/cookie": "^0.4.1",
        "@types/js-cookie": "^3.0.0",
        "@types/worker-plugin": "^5.0.1",
        "@vue/test-utils": "^1.1.3",
        "apollo-link": "^1.2.14",
        "axios": "^0.26.1",
        "cross-env": "^7.0.2",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.1.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-nuxt": "^3.0.0",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "^7.0.2",
        "lint-staged": "^12.0.1",
        "nuxt-storm": "^1.1.3",
        "prettier": "^2.1.1",
        "pusher-js": "5.1.1",
        "require-extension-hooks": "^0.3.3",
        "require-extension-hooks-babel": "^1.0.0",
        "require-extension-hooks-vue": "^3.0.0",
        "stylelint": "^13.13.1",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-standard": "^22.0.0",
        "typescript": "^4.6.3",
        "vue-styleguidist": "^4.44.22",
        "vue-template-babel-compiler": "^1.0.7",
        "worker-plugin": "^5.0.0"
    },
    "peerDependencies": {
        "vue": "^2.6.14"
    }
